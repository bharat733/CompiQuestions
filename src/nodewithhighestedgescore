class Solution {
    public int edgeScore(int[] edges) {
         long n= edges.length;
       long[] arr=new long[(int) n];
        long ans=0;
        for(int i=0;i<n;i++){
            arr[edges[i]]+=i;
        }

        long maxi=-1;
        for(int i=0;i<n;i++){
            if(maxi<arr[i]){
                ans=i;
                maxi= arr[i];
            }
        }
        return (int) ans;
    }
}
